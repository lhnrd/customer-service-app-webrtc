{
  "name": "sac-api-server",
  "version": "0.0.0",
  "private": true,
  "main": "src",
  "repository": "git@github.com:lcsrinaldi/sac-api.git",
  "scripts": {
    "coverage": "npm test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "dev": "nodemon -i \"*.test.js\" .",
    "docs": "apidoc -i src -o docs && apidoc-markdown -p docs -o DOCS.md",
    "postdocs": "opn docs/index.html",
    "km": "knex-migrate --knexfile src/config/knexfile.js",
    "km:test": "NODE_ENV=test yarn km",
    "km:prod": "NODE_ENV=production yarn km",
    "ks": "knex --knexfile src/config/knexfile.js",
    "ks:test": "NODE_ENV=test yarn ks",
    "ks:prod": "NODE_ENV=production yarn ks",
    "lint": "eslint src",
    "prod": "cross-env NODE_ENV=production nodemon -i \"*.test.js\" -r dotenv-safe/config .",
    "start": "node .",
    "test": "jest -i",
    "test:watch": "yarn test --watch"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "testEnvironment": "node"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "date-fns": "^2.0.0-alpha.23",
    "debug": "^4.1.0",
    "express": "^4.16.2",
    "joi": "^13.4.0",
    "jsonwebtoken": "^8.1.0",
    "knex": "^0.14.6",
    "lodash": "^4.17.11",
    "lodash.pick": "^4.4.0",
    "morgan": "^1.7.0",
    "objection": "^1.1.10",
    "objection-filter": "^1.4.0",
    "objection-guid": "^3.0.1",
    "objection-password": "^1.0.0",
    "objection-timestamps": "^3.0.2",
    "objection-visibility": "^0.3.2",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^3.0.0",
    "pg": "^7.4.3",
    "rand-token": "^0.4.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "socket.io": "^2.1.1",
    "standard": "^11.0.1"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "apidoc-markdown": "^0.2.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.0.5",
    "dotenv-safe": "^4.0.3",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "faker": "^4.1.0",
    "jest": "^23.2.0",
    "joi-date-extensions": "^1.2.0",
    "knex-migrate": "^1.7.0",
    "nock": "^9.0.2",
    "nodemon": "^1.10.2",
    "opn-cli": "^3.1.0",
    "sinon": "^4.0.1",
    "socket.io-client": "^2.1.1",
    "supertest": "^3.0.0"
  }
}
